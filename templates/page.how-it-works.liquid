<div class="page-width">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
        </div>
        <ul class="grid grid--uniform grid--view-items hiw-items">
            
            <li class="grid__item  small--one-half medium-up--one-third" style="text-align: center;">
                <i class="las la-trophy"></i>
                <h3>Choose Your Desired Prize</h3>
                <p>Choose one of our prizes.<p>        
            </li>
            
            <li class="grid__item  small--one-half medium-up--one-third" style="text-align: center;">
                <i class="las la-ticket-alt"></i>
                <h3>Purchase a Wizza Ticket</h3>
                <p>Enter a giveaway for as low as $0.05 per ticket!<p>                
            </li>
            
            <li class="grid__item  small--one-half medium-up--one-third" style="text-align: center;">
                <i class="las la-clock"></i>
                <h3>Await for The Wizza</h3>
                <p>Wait until The Wizza makes his magic happen.<p>      
            </li>
            
        </ul>
      <div class="rte">
        {{ page.content }}
      </div>
    </div>
  </div>
  {% section 'collection' %}
</div>

